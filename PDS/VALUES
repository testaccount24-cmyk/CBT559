/* values.c */
/*******************************************/
/* tests function which accepts two values */
/*******************************************/
#include <stdio.h>

void gets2(int,int);

int main() {
  int x=4,y=7;

  gets2(x,y);
}

void gets2(int xx,int yy) {
  printf("First is %d, second is %d",xx,yy);
}
