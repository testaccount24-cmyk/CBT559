/**************/
/* printall.c */
/******************************************************************/
/* displays all possible hex characters, 0-255                    */
/* author r thornton September 1994                               */
/******************************************************************/
#include <stdio.h>
void main(void) {
  unsigned char c;
  for(c=0; c<=255; c++) {
    printf("\nchar=%c, hex=%02X, decimal=%03d",c,c,c);
    getchar();
  }
}
