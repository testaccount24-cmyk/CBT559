/* kbdtest.c */
/*******************************/
/* prints code of keyboard key */
/*******************************/
#include <stdio.h>
int  main() {
  char key,key2;

  while((key=getchar()) != 'X')
  if(key == 0) {
    key2=getchar();
    printf("%3d %3d\n",key,key2);
  }
  else
    printf("%3d\n",key);
}
