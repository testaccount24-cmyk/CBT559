/*************/
/* testbox.c */
/***************************************/
/* puts box around text using function */
/***************************************/
#include <stdio.h>

void line(void);

main() {
  line();
  printf("\n\xDB Titus Andronicus \xDB");
  line();
}

void line(void) {
  int cnt;

  putchar('\n');
  for(cnt=0;cnt<20;cnt++)
    putchar('\xDB');
}
