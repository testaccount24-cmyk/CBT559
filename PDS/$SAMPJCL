//COMPBIND EXEC CCOMP,CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3GRE1)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3GRE1)'
//BIND.SYSIN DD *
 ENTRY CEESTART
 NAME CBC3GRE1(R)
//*******************************************************************
//* EXECUTE THE SAMPLE PROGRAM                                      *
//*******************************************************************
//CBC3GRE1 EXEC  PGM=CBC3GRE1
//STEPLIB  DD    DSN=CBC.TRIGON.SAMPLOD,DISP=SHR
//         DD    DSN=CEE.SCEERUN,DISP=SHR
//SYSUDUMP DD    SYSOUT=*
//SYSPRINT DD    SYSOUT=*
./ ADD NAME=CBC3GRE3 0106-00259-00277-1524-00036-00033-00000-BC0THOR
//BC0THORH JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=100M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//*******************************************************************
//*                 CBC.TRIGON.SAMPJCL(CBC3GRE3)                    *
//* ASSEMBLE/LINK FOR C LANGUAGE SAMPLE ASSEMBLER PROGRAM CBC3GRE3  *
//* !!!!! ==> MUST BE LINKED INTO A PDSE TO FUNCTION <== !!!!!      *
//* !!!!! ==> PROPERLY WITH CBC3GRE4. CURRENTLY HAS  <== !!!!!      *
//* !!!!! ==> A PROBLEM UNDER IBM INVESTIGATION.     <== !!!!!      *
//*******************************************************************
//*******************************************
//*  ASSEMBLY                               *
//*******************************************
//ASM      EXEC PGM=ASMA90,
//  PARM=(OBJECT,'XREF(FULL)',ESD,LIST,'FLAG(NORECORD)')
//SYSLIB   DD DSN=SYS1.MACLIB,DISP=SHR
//         DD DSN=CEE.SCEEMAC,DISP=SHR
//SYSUT1   DD DSN=&&SYSUT1,UNIT=DISK,SPACE=(CYL,(10,5))
//SYSPRINT DD SYSOUT=*
//SYSUDUMP DD SYSOUT=*
//SYSPUNCH DD DUMMY,DCB=BLKSIZE=80
//SYSLIN   DD DSN=&&LOADSET,DISP=(,PASS),
//         UNIT=DISK,SPACE=(CYL,(5,1),RLSE),
//         BLKSIZE=400
//SYSIN    DD DSN=CBC.TRIGON.SAMPSRC(CBC3GRE3),DISP=SHR
//*******************************************
//*  LINKEDIT                               *
//*******************************************
//LKED    EXEC PGM=IEWL,REGION=256K,COND=(8,LT,ASM),
//  PARM='XREF,LET,LIST,NCAL,MAP,SIZE=(256K,96K)'
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD DSN=&&LOADSET,DISP=(SHR,DELETE,DELETE)
//SYSUT1   DD UNIT=DISK,SPACE=(CYL,(1,1))
//SYSLIB   DD DSN=SYS1.LINKLIB,DISP=SHR
//SYSLMOD  DD DSN=BC0THOR.PDSE.LOAD(CBC3GRE3),DISP=SHR
./ ADD NAME=CBC3GRE4 0107-00263-00277-1709-00054-00051-00000-BC0THOR
//BC0THORD JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=104M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//********************************************************************
//*                CBC.TRIGON.SAMPJCL(CBC3GRE4)                      *
//* !!!!! ==> MUST BE LINKED INTO A PDSE TO FUNCTION <== !!!!!       *
//* !!!!! ==> PROPERLY WITH CBC3GRE3. CURRENTLY HAS  <== !!!!!       *
//* !!!!! ==> A PROBLEM UNDER IBM INVESTIGATION.     <== !!!!!       *
//********************************************************************
//********************************************************************
//*  CONVERT SQUARE BRACKETS TO COMPILER EXPECTED VALUES             *
//********************************************************************
//CBRAKETS EXEC PGM=CBRAKETS
//STEPLIB  DD   DSN=CBC.TRIGON.SAMPLOD,DISP=SHR
//SYSUDUMP DD   SYSOUT=*
//SYSIN    DD   DSN=CBC.TRIGON.SAMPSRC(CBC3GRE4),DISP=SHR
//SYSOUT   DD   DSN=&&CSOURCE,DISP=(,PASS),
//         UNIT=DISK,SPACE=(CYL,(2,1),RLSE),
//         DSORG=PS,RECFM=FB,LRECL=80
//********************************************************************
//*  COMPILE THE SAMPLE PROGRAM                                      *
//********************************************************************
//COMPILE EXEC PGM=CBCDRVR,
//    PARM=('/AGG,EXP,LIS,OF,OPT,SO,XR,CHE(ALL)')
//STEPLIB  DD  DSN=CEE.SCEERUN,DISP=SHR
//         DD  DSN=CBC.SCBCCMP,DISP=SHR
//SYSLIB   DD  DSN=CEE.SCEEH.H,DISP=SHR
//         DD  DSN=CEE.SCEEH.SYS.H,DISP=SHR
//         DD  DSN=CBC.SCBCSAM,DISP=SHR
//SYSLIN   DD  DSN=&&LOADSET,DISP=(NEW,PASS),
//         UNIT=DISK,SPACE=(TRK,(3,3)),
//         RECFM=FB,LRECL=80,BLKSIZE=3200
//SYSPRINT DD  SYSOUT=*
//SYSOUT   DD  SYSOUT=*
//SYSCPRT  DD  SYSOUT=*
//SYSIN    DD  DSN=&&CSOURCE,DISP=(OLD,PASS,DELETE)
//********************************************************************
//*  BIND THE SAMPLE PROGRAM WITH CALL                               *
//********************************************************************
//BIND   EXEC PGM=IEWL,COND=(4,LT),
//  PARM='XREF,LET,LIST,CALL,MAP,SIZE=(256K,96K)'
//STEPLIB  DD  DSN=CEE.SCEERUN,DISP=SHR
//SYSLIB   DD  DSN=CEE.SCEELKED,DISP=SHR
//         DD  DSN=BC0THOR.PDSE.LOAD,DISP=SHR
//SYSPRINT DD  SYSOUT=*
//SYSLIN   DD  DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLMOD  DD  DSN=BC0THOR.PDSE.LOAD(CBC3GRE4),DISP=SHR
//SYSDEFSD DD  DUMMY
//SYSIN    DD  *
 INCLUDE SYSLIB(CBC3GRE3)
 ENTRY CEESTART
 NAME CBC3GRE4(R)
./ ADD NAME=CBC3GTH1 0102-00266-00277-1709-00034-00018-00000-BC0THOR
//BC0THORJ JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=104M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//MYPROCS JCLLIB ORDER=CBC.TRIGON.SAMPJCL
//********************************************************************
//*                 CBC.TRIGON.SAMPJCL(CBC3GTH1)                     *
//* USES THREAD-SPECIFIC DATA TO INSURE THAT STORAGE ACQUIRED BY A   *
//* SPECIFIC THREAD IS FREED WHEN THE THREAD ENDS.                   *
//* NOTE THAT THE OUTPUT FROM THE PRINTF FUNCTION IS WRITTEN TO THE  *
//* SYSPRINT DD STATEMENT, IF IT IS SUPPLIED IN THE EXECUTION JCL. IF*
//* NOT, THE PRINTF OUTPUT IS DYNAMICALLY ALLOCATED TO A SYSOUT=A    *
//* FILE.                                                            *
//* 1. COMPILE PROGRAM CBC3GTH1 AND BIND CALL AS CBC3GTH1            *
//* 2. EXECUTE FINAL LOAD MODULE FOR CBC3GTH1. PRODUCES NO OUTPUT,   *
//*    BUT ENDS RC=0.                                                *
//********************************************************************
//*******************************************************************
//* COMPILE AND BIND THE SAMPLE PROGRAM                             *
//*******************************************************************
//COMPBIND EXEC CCOMP,CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3GTH1)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3GTH1)'
//BIND.SYSIN DD *
 ENTRY CEESTART
 NAME CBC3GTH1(R)
//*******************************************************************
//* EXECUTE THE SAMPLE PROGRAM                                      *
//*******************************************************************
//CBC3GTH1 EXEC  PGM=CBC3GTH1
//STEPLIB  DD    DSN=CBC.TRIGON.SAMPLOD,DISP=SHR
//         DD    DSN=CEE.SCEERUN,DISP=SHR
//SYSUDUMP DD    SYSOUT=*
//SYSPRINT DD    SYSOUT=*
./ ADD NAME=CBC3GVS1 0104-00265-00277-1710-00095-00076-00000-BC0THOR
//BC0THORD JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=104M
//JESOUT OUTPUT GROUPID=HDQTRSF2,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//MYPROCS JCLLIB ORDER=CBC.TRIGON.SAMPJCL
//*******************************************************************
//*               CBC.TRIGON.SAMPJCL(CBC3GVS1)                      *
//* THIS EXAMPLE SHOWS HOW TO ACCESS THE __AMRC->__RBA FIELD.       *
//* IT ASSUMES THAT AN ESDS HAS ALREADY BEEN DEFINED, AND HAS BEEN  *
//* ASSIGNED THE DDNAME ESDSCLUS.                                   *
//* 1. COMPILE PROGRAM CBC3GVS1 AND BIND CALL AS CBC3GVS1           *
//* 2. DEFINE THE VSAM ESDS                                         *
//* 3. REPRO DATA INTO THE VSAM ESDS                                *
//* 4. EXECUTE FINAL LOAD MODULE FOR CBC3GVS1. PROGRAM RETURNS ERROR*
//*    MESSAGES FOLLOWING FOPEN() CALL, BUT ENDS WITH RC=0.         *
//* 5. PRINT THE VSAM ESDS                                          *
//* 6. DELETE THE VSAM ESDS                                         *
//*******************************************************************
//*******************************************************************
//* COMPILE AND BIND THE SAMPLE PROGRAM                             *
//*******************************************************************
//COMPBIND EXEC CCOMP,CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3GVS1)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3GVS1)'
//BIND.SYSIN DD *
 ENTRY CEESTART
 NAME CBC3GVS1(R)
//*******************************************************************
//* DEFINE ESDS                                                     *
//*******************************************************************
//DEFINE  EXEC PGM=IDCAMS
//VOLUME    DD UNIT=DISK,DISP=SHR,VOL=SER=WORK01
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
    DEFINE CLUSTER -
        (NAME(TEST.TSTESDS) -
         FILE(VOLUME) -
         VOLUMES(WORK01) -
         NONINDEXED -
         CYLINDERS(5 1) -
         RECORDSIZE(80 80) -
         FREESPACE(10 10) -
         CISZ(4096) -
         SHR(3,3) -
         SPEED -
         UNIQUE -
         NOERASE -
         NOWRITECHECK -
         UNORDERED -
         NOREUSE -
         NONSPANNED -
        )  -
      DATA -
        (NAME(TEST.TSTESDS.DATA))
//*******************************************************************
//* REPRO DATA INTO ESDS                                            *
//*******************************************************************
//REPRO   EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
   REPRO INDATASET(CBC.TRIGON.SAMPSRC(CBC3GVS1)) -
      OUTDATASET(TEST.TSTESDS)
//*******************************************************************
//* EXECUTE THE TEST PROGRAM CBC3GVS1                               *
//*******************************************************************
//CBC3GVS1  EXEC PGM=CBC3GVS1,REGION=5M
//STEPLIB  DD DSN=CBC.TRIGON.SAMPLOD,DISP=SHR
//         DD DSN=CEE.SCEERUN,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//SYSTERM   DD SYSOUT=*
//SYSOUT    DD SYSOUT=*
//PLIDUMP   DD SYSOUT=*
//SYSABEND  DD SYSOUT=*
//SYSUDUMP  DD SYSOUT=*
//ESDSCLUS  DD DSN=TEST.TSTESDS,DISP=SHR
//*******************************************************************
//* PRINT OUT THE ESDS CLUSTER                                      *
//*******************************************************************
//PRINTF    EXEC PGM=IDCAMS
//SYSPRINT   DD SYSOUT=*
//SYSIN      DD *
   PARM GRAPHICS(CHAIN(SN))
   PRINT -
       INDATASET(TEST.TSTESDS) CHAR
//*******************************************************************
//* DELETE THE ESDS CLUSTER                                         *
//*******************************************************************
//DELETEC EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
    DELETE -
        TEST.TSTESDS -
        CLUSTER -
        PURGE -
        ERASE
./ ADD NAME=CBC3GVS2 0106-00264-00277-1711-00135-00123-00000-BC0THOR
//BC0THORD JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=104M
//JESOUT OUTPUT GROUPID=HDQTRSF2,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//MYPROCS JCLLIB ORDER=CBC.TRIGON.SAMPJCL
//*******************************************************************
//*               CBC.TRIGON.SAMPJCL(CBC3GVS2)                      *
//* THIS EXAMPLE DEMONSTRATES THE USE OF A KSDS FILE.               *
//* 1. COMPILE PROGRAM CBC3GVS2 AND BIND CALL AS CBC3GVS2           *
//* 2. DEFINE THE VSAM KSDS                                         *
//* 3. REPRO DATA INTO THE KSDS                                     *
//* 4. DEFINE UNIQUE ALTERNATE INDEX AND BUILD PATH                 *
//* 5. DEFINE NONUNIQUE ALTERNATE INDEX AND BUILD PATH              *
//* 6. EXECUTE FINAL LOAD MODULE FOR CBC3GVS2                       *
//* 7. PRINT THE VSAM KSDS                                          *
//* 8. DELETE THE VSAM KSDS                                         *
//*******************************************************************
//*******************************************************************
//* COMPILE AND BIND THE SAMPLE PROGRAM                             *
//*******************************************************************
//COMPBIND EXEC CCOMP,CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3GVS2)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3GVS2)'
//BIND.SYSIN DD *
 ENTRY CEESTART
 NAME CBC3GVS2(R)
//*******************************************************************
//* DEFINE KSDS                                             *
//*******************************************************************
//DEFINE  EXEC PGM=IDCAMS
//VOLUME    DD UNIT=DISK,DISP=SHR,VOL=SER=WORK01
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
    DEFINE CLUSTER -
        (NAME(TEST.TSTKSDS) -
        FILE(VOLUME) -
        VOL(WORK01) -
        TRK(4 4)  -
        RECSZ(69 100) -
        INDEXED -
        NOREUSE -
        KEYS(4 0) -
        OWNER(TEST) ) -
      DATA -
        (NAME(TEST.TSTKSDS.DATA)) -
      INDEX -
        (NAME(TEST.TSTKSDS.INDEX))
//*******************************************************************
//* REPRO DATA INTO KSDS                                            *
//*******************************************************************
//REPRO   EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
   REPRO INDATASET(CBC.TRIGON.SAMPSRC(CBC3GVS3)) -
      OUTDATASET(TEST.TSTKSDS)
//*******************************************************************
//* DEFINE UNIQUE AIX, DEFINE AND BUILD PATH                        *
//*******************************************************************
//DEFAIX  EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
    DEFINE AIX -
        (NAME(TEST.TSTKSDS.UAIX) -
        RECORDS(25)  -
        KEYS(8,4)    -
        VOL(WORK01)  -
        UNIQUEKEY -
        RELATE(TEST.TSTKSDS)) -
      DATA -
        (NAME(TEST.TSTKSDS.UAIXDA)) -
      INDEX -
        (NAME(TEST.TSTKSDS.UAIXIX))
    DEFINE PATH -
        (NAME(TEST.TSTKSDS.UPATH) -
        PATHENTRY(TEST.TSTKSDS.UAIX))
    BLDINDEX -
        INDATASET(TEST.TSTKSDS) -
        OUTDATASET(TEST.TSTKSDS.UAIX)
//*******************************************************************
//* DEFINE NONUNIQUE AIX, DEFINE AND BUILD PATH                     *
//*******************************************************************
//DEFAIX2 EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
    DEFINE AIX -
        (NAME(TEST.TSTKSDS.NUAIX) -
        RECORDS(25)  -
        KEYS(20, 12)    -
        VOL(WORK01)  -
        NONUNIQUEKEY -
        RELATE(TEST.TSTKSDS)) -
      DATA -
        (NAME(TEST.TSTKSDS.NUAIXDA)) -
      INDEX -
        (NAME(TEST.TSTKSDS.NUAIXIX))
    DEFINE PATH -
        (NAME(TEST.TSTKSDS.NUPATH) -
        PATHENTRY(TEST.TSTKSDS.NUAIX))
    BLDINDEX -
        INDATASET(TEST.TSTKSDS) -
        OUTDATASET(TEST.TSTKSDS.NUAIX)
//*******************************************************************
//* RUN THE TESTCASE                                                *
//*******************************************************************
//GO        EXEC PGM=CBC3GVS2,REGION=5M
//STEPLIB  DD DSN=CBC.TRIGON.SAMPLOD,DISP=SHR
//         DD DSN=CEE.SCEERUN,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//SYSTERM   DD SYSOUT=*
//SYSOUT    DD SYSOUT=*
//PLIDUMP   DD SYSOUT=*
//SYSABEND  DD SYSOUT=*
//SYSUDUMP  DD SYSOUT=*
//CLUSTER   DD DSN=TEST.TSTKSDS,DISP=SHR
//AIXUNIQ   DD DSN=TEST.TSTKSDS.UPATH,DISP=SHR
//AIXNUNIQ  DD DSN=TEST.TSTKSDS.NUPATH,DISP=SHR
//*******************************************************************
//* PRINT OUT THE CLUSTER                                           *
//*******************************************************************
//PRINTF    EXEC PGM=IDCAMS
//SYSPRINT   DD SYSOUT=*
//SYSIN      DD *
   PRINT -
       INDATASET(TEST.TSTKSDS) CHAR
//*******************************************************************
//* DELETE CLUSTER, AND AIX AND PATH                                *
//*******************************************************************
//DELETEC EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
    DELETE -
        TEST.TSTKSDS -
        CLUSTER -
        PURGE -
        ERASE
./ ADD NAME=CBC3GVS4 0103-00265-00277-1328-00085-00068-00000-BC0THOR
