//*******************************************************************
//COMPBIND EXEC CCOMP,CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3GVS4)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3GVS4)'
//BIND.SYSIN DD *
 ENTRY CEESTART
 NAME CBC3GVS4(R)
//*******************************************************************
//* DEFINE THE RRDS CLUSTER                                         *
//*******************************************************************
//DEFINE   EXEC PGM=IDCAMS
//VOLUME   DD   UNIT=DISK,DISP=SHR,VOL=SER=WORK01
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
    DEFINE CLUSTER -
        (NAME(TEST.TSTRRDS.CLUSTER) -
         FILE(VOLUME) -
         VOL(WORK01) -
         NUMBERED -
         TRK(5 1)  -
         RECSZ(80 80) -
         REUSE -
         FREESPACE(10 10) -
         SHR(3,3) -
         SPEED -
        ) -
      DATA -
        (NAME(TEST.TSTRRDS.DA))
//*******************************************************************
//* REPRO DATA INTO THE RRDS CLUSTER                                *
//*******************************************************************
//REPRO    EXEC PGM=IDCAMS
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
   REPRO INDATASET(CBC.TRIGON.SAMPSRC(CBC3GVS4)) -
      OUTDATASET(TEST.TSTRRDS.CLUSTER)
//*******************************************************************
//* EXECUTE THE CBC3GVS4 TEST PROGRAM                               *
//*******************************************************************
//CBC3GVS4 EXEC PGM=CBC3GVS4,REGION=5M
//STEPLIB  DD   DSN=CBC.TRIGON.SAMPLOD,DISP=SHR
//         DD   DSN=CEE.SCEERUN,DISP=SHR
//SYSPRINT DD   SYSOUT=*
//SYSTERM  DD   SYSOUT=*
//SYSOUT   DD   SYSOUT=*
//PLIDUMP  DD   SYSOUT=*
//SYSABEND DD   SYSOUT=*
//SYSUDUMP DD   SYSOUT=*
//RRDSFILE DD   DSN=TEST.TSTRRDS.CLUSTER,DISP=SHR
//*******************************************************************
//* PRINT OUT THE RRDS CLUSTER                                      *
//*******************************************************************
//PRINTF   EXEC PGM=IDCAMS
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
   PARM GRAPHICS(CHAIN(SN))
   PRINT INDATASET(TEST.TSTRRDS.CLUSTER) CHAR
//*******************************************************************
//* DELETE THE RRDS CLUSTER                                         *
//*******************************************************************
//DELETEC EXEC PGM=IDCAMS
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
    DELETE -
        TEST.TSTRRDS.CLUSTER -
        CLUSTER -
        PURGE -
        ERASE
