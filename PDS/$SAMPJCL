//BC0THORD JOB (1152002-9800039,,TMVS01-UR-XXXX),'DTHORNTON',CLASS=V,
//   MSGCLASS=Y,NOTIFY=BC0THOR,REGION=104M
//JESOUT OUTPUT GROUPID=02A,DEFAULT=YES,JESDS=ALL,
//   PAGEDEF=GRYBIG,FORMDEF=GRYBIG
//MYPROCS JCLLIB ORDER=CBC.TRIGON.SAMPJCL
//*******************************************************************
//*                 CBC.TRIGON.SAMPJCL(CBC3RAH1)                    *
//* THIS EXAMPLE SHOWS THE LINKAGE OF EXTERN OBJECTS/FUNCTIONS.     *
//* 1. COMPILE SUB PROGRAM CBC3RAH3 AND BIND NCAL AS TOTAL          *
//* 2. COMPILE SUB PROGRAM CBC3RAH2 AND BIND NCAL AS CBC3RAH2       *
//* 3. COMPILE MAIN PROGRAM CBC3RAH1 AND BIND INCLUDING SUB-PROGRAMS*
//*    CBC3RAH3, AND CBC3RAH2 WITH CALL AS CBC3RAH1.                *
//* 4. EXECUTE PROGRAM CBC3RAH1 PROVIDING PRICE INPUT VIA SYSIN.    *
//*******************************************************************
//*******************************************************************
//* COMPILE AND BIND THE SUBPROGRAM NCAL AS TOTAL                   *
//*******************************************************************
//COMPBIN1 EXEC CCOMP,CALL=NCAL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3RAH3)',
//   LOAD='CBC.TRIGON.SAMPLOD(TOTAL)'
//*******************************************************************
//* COMPILE AND BIND THE SUBRPOGRAM AS CBC3RAH2                     *
//*******************************************************************
//COMPBIN2 EXEC CCOMP,CALL=NCAL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3RAH2)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3RAH2)'
//*******************************************************************
//* COMPILE AND BIND THE MAIN PROGRAM CALL AS CBC3RAH1              *
//*******************************************************************
//COMPBND2 EXEC CCOMP,COND=(4,LT),CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3RAH1)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3RAH1)'
//BIND.SYSIN DD *
 INCLUDE MYLOAD(CBC3RAH2)
 ENTRY CEESTART
