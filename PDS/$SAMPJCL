//COMPBIND EXEC CCOMP,CALL=CALL,
//   SRC='CBC.TRIGON.SAMPSRC(CBC3GOP2)',
//   LOAD='CBC.TRIGON.SAMPLOD(CBC3GOP2)'
//BIND.SYSIN DD *
 ENTRY CEESTART
